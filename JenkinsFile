pipeline {
    parameters {
        string(name: 'v1')
        string(name: 'v2')
        file(name:'asdasdad')
    }
    options {
        skipDefaultCheckout(true)
    }
    agent('any')
    stages {
        stage('Creating-Accounts'){
            steps{
                checkout([$class: 'GitSCM',branches:[[name:'*/main']],doGenerateSubmoduleConfigurations:false,userRemoteConfigs:[[url:'https://github.com/manoharc07/scratch.git']]])
               bat '''
                    echo "Testing"
               '''
            
            }
        }
        
    }
}
